<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <GeneDef Name="GeneGrowthRateBase" Abstract="True">
    <displayCategory>GrowthRate</displayCategory>
  </GeneDef>

  <GeneDef ParentName="GeneGrowthRateBase">
    <defName>VeryFastGrowthRate</defName>
    <label>Very fast growth rate</label>
    <labelShortAdj>Very fast growth rate</labelShortAdj>
    <description>Carriers of this gene grow very fast in their childhood.</description>
    <iconPath>UI/Icons/Genes/Gene_TotalHealing</iconPath>
    <displayOrderInCategory>10</displayOrderInCategory>
    <exclusionTags>
      <li>YouthAging</li>
    </exclusionTags>
    <statFactors>
      <GlobalLearningFactor>0.25</GlobalLearningFactor>
    </statFactors>
    <biologicalAgeTickFactorFromAgeCurve>
      <points>
        <li>(3, 15)</li>
        <li>(4, 10)</li>
        <li>(13, 5)</li>
      </points>
    </biologicalAgeTickFactorFromAgeCurve>
    <biostatCpx>1</biostatCpx>
    <biostatMet>-2</biostatMet>
  </GeneDef>

  <GeneDef ParentName="GeneGrowthRateBase">
    <defName>FastGrowthRate</defName>
