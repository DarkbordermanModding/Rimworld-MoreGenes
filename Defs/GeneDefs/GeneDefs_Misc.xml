<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <!-- Great memory -->
  <GeneDef>
    <defName>GreatMemory</defName>
    <label>Great Memory</label>
    <labelShortAdj>Great Memory</labelShortAdj>
    <description>Carriers of this gene have excellent memories.</description>
    <iconPath>UI/Icons/Genes/Gene_FastLearning</iconPath>
    <displayOrderInCategory>60</displayOrderInCategory>
    <forcedTraits>
      <li>
        <def>GreatMemory</def>
      </li>
    </forcedTraits>
    <biostatCpx>1</biostatCpx>
    <biostatMet>-1</biostatMet>
  </GeneDef>

  <!-- Cannibal -->
  <GeneDef>
    <defName>Cannibal</defName>
    <label>Cannibal</label>
    <labelShortAdj>Cannibal</labelShortAdj>
    <description>Carriers of this gene have instinct to human meat.</description>
    <iconPath>UI/Icons/Genes/Gene_Bloodfeeder</iconPath>
    <displayOrderInCategory>65</displayOrderInCategory>
    <forcedTraits>
      <li>
        <def>Cannibal</def>
      </li>
    </forcedTraits>
    <biostatCpx>1</biostatCpx>
    <biostatMet>0</biostatMet>
  </GeneDef>

  <!-- Bloodlust -->
  <GeneDef>
    <defName>Aggression_Bloodlust</defName>
    <label>Bloodlust</label>
    <labelShortAdj>Bloodlust</labelShortAdj>
    <description>Carriers of this gene have instinct to kill others.</description>
    <iconPath>UI/Icons/Genes/Gene_Killthirst</iconPath>
    <displayCategory>Violence</displayCategory>
    <displayOrderInCategory>90</displayOrderInCategory>
    <forcedTraits>
      <li>
        <def>Bloodlust</def>
      </li>
    </forcedTraits>
    <biostatCpx>1</biostatCpx>
    <biostatMet>1</biostatMet>
  </GeneDef>
</Defs>
